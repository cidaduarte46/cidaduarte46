  programa {
	funcao inicio() {    
	
	
	real Mmatematica[3][5], Mportugues [3][5], Mhistoria[3][5], Mingles[3][5], Mfisica[3][5]
	cadeia Mnome[3][5]
	inteiro  Mindice,Mturma
	Mindice = 0
	
	para (Mturma = 0; Mturma <= 2; Mturma ++)
	{
	    para(Mindice = 0; Mindice <=4; Mindice ++)
	    {
    	    escreva ("digite um nome: ")
    		leia (Mnome[Mturma][Mindice])
    		escreva("digite a nota de portugues:")
    	    leia (Mmatematica[Mturma][Mindice])
    	    escreva("digite a nota de biologia:")
    	    leia(Mportugues [Mturma][Mindice]) 
    	    escreva ("digite a nota de histÃ³ria:")
    	    leia(Mhistoria [Mturma][Mindice])
    	    escreva("digite a nota de arte:")
    	    leia(Mingles[Mturma][Mindice])
    	    escreva("digite a nota de fisica:")
    	    leia(Mfisica[Mturma][Mindice])
	    }
	}
	para (Mturma=0; Mturma<=2; Mturma++)
	              {  
		                escreva("\nAlunos da turma: ", Mturma)
		        
		                para(Mindice=0;Mindice<=4;Mindice++)
	                    {
	                       escreva("\nAluno: ", Mnome[Mturma][Mindice])
	                       se (Mingles[Mturma][Mindice]>60)
	                       escreva("\naprovado em biologia")
	                       senao
	                       escreva("\nreprovado em biologia")
	                   
	                       se (Mmatematica[Mturma][Mindice]>60)
	                       escreva("\naprovado em matematica")
	                       senao
	                       escreva("\nreprovado em matematica")
	                   
	                       se (Mhistoria[Mturma][Mindice]>60)
	                       escreva("\naprovado em historia")
	                       senao
	                       escreva("\nreprovado em historia")
	                  
	                       se (Mfisica[Mturma][Mindice]>60)
	                       escreva("\naprovado em artes")
	                       senao
	                       escreva("\nreprovado em artes")
	                   
	                       se(Mportugues[Mturma][Mindice]>60)    
	                       escreva("\naprovado em portugues")
	                       senao
	                       escreva("\nreprovado em portugues")
	                    }
		       }     
	    } 
}
